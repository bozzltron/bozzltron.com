<header class="blog-header ">
  <div class="inner">
    <div class="box blog-box">
      <h1 class="blog-name">@bozzltron</h1>
      <span class="blog-description">A Message In A Bottle</span>
    </div>
  </div>
</header>

<div id="index" class="container">

  <main class="content" role="main">

    <div class="extra-pagination">
      <nav class="pagination">
        <div class="inner">
          <div class="box pagination-box" role="navigation" aria-label="Pagination Navigation">
            <span class="pagination-info">Page 1 of 1</span>
          </div>
        </div>
      </nav>
    </div>
    <% var last; %>
    <% page.posts.each(function(post, i){ %>
    <% var year = post.date.year(); %>
    <% if (last != year){ %>
    <% if (last != null){ %>
    <% } %>
    <% last = year; %>
    <% } %>
    <%- partial('archive-post', {post: post, even: i % 2 == 0}) %>
    <% }) %>
    <% if (page.posts.length){ %>
    <% } %>
  </main>
</div>


<% if (page.total > 1){ %>
<nav class="pagination">
  <div class="inner">
    <div class="box pagination-box" role="navigation" aria-label="Pagination Navigation">
      <span class="pagination-info">
        <% var prev_text = "&laquo; " + __('prev');var next_text = __('next') + " &raquo;"%>
        <%- paginator({
            prev_text: prev_text,
            next_text: next_text
          }) %>
      </span>
    </div>
  </div>
</nav>
<% } %>