<div class="nav-header">
  <nav class="nav-wrapper">
    <ul role="navigation" aria-label="Navigation">
      <li class="nav-home <% if (path === 'index.html'){ %>active<% } %>" role="presentation"><a
          href="<%- url_for() %>"><span>Home</span></a></li>
      <li class="nav-archives <% if (path === 'archives/index.html'){ %>active<% } %>" role="presentation"><a
          href="/archives/"><span>Archives</span></a></li>
    </ul>

    <ul class="nav-side">
      <li class="nav-twitter"><a title="Twitter" href="https://twitter.com/bozzltron" target="_blank"><i
            class="icon icon-twitter"></i><span>Twitter</span></a></li>
      <li class="nav-github"><a title="Github" href="https://www.github.com/bozzltron" target="_blank"><i
            class="icon icon-github"></i><span>Github</span></a></li>
      <li class="nav-linkedin"><a title="Github" href="https://www.linkedin.com/in/michael-bosworth-11084324/"
          target="_blank"><i class="icon icon-linkedin"></i><span></span></a></li>
      <% if (theme.rss){ %>
      <li class="nav-rss">
        <a id="nav-rss-link" class="nav-icon" href="<%- url_for(theme.rss) %>" title="<%= __('rss_feed') %>">
          <i class="icon icon-rss"></i>
        </a>
      </li>
      <% } %>
      <li class="nav-search" style="display: none;"><a title="Search"><i
            class="icon icon-search"></i><span>Search</span></a></li>
    </ul>
  </nav>

  <div class="nav-wrapper-control">
    <div class="inner">
      <a class="nav-menu"><i class="icon icon-menu"></i>Menu</a>
      <a class="nav-search" style="display: none;" title="Search"><i
          class="icon icon-search"></i><span>Search</span></a>
    </div>
  </div>
</div>
<div class="nav-close"></div>

<section class="page-wrapper">
  <!-- Tag ends in footer -->